{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/almuerzos-peru-front/browser",
  "framework": null,
  "functions": {
    "api/angular.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/angular.js"
    }
  ]
}
